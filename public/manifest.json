{
  "name": "Salesforce API Inventory",
  "description": "Salesforce non-official extension to map API's integration inside a organization",
  "version": "0.0.1",
  "manifest_version": 3,
  "icons": {
     "32": "logo_icon.png"
   },
  "permissions": [
      "activeTab",
      "scripting"
   ],
   "action": {
      "default_popup": "index.html"
   },
   "host_permissions": [
      "https://*/*",
      "https://*/"
   ]
}

